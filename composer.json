{
    "name": "spiral/auth-http",
    "license": "MIT",
    "description": "Authorization component with set of authentication middlewares and providers",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/auth"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "spiral/auth": "^2.9",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "spiral/cookies": "^2.9",
        "spiral/http": "^2.9",
        "spiral/debug": "^2.9",
        "laminas/laminas-diactoros": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Auth\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.9.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
